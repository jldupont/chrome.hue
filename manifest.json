{
  "manifest_version": 2,

  "name": "Chrome.Hue",
  "description": "Communication with LAN local Philips Hue hub",
  "version": "0.2.0"

  ,"background": {
      "scripts": ["chrome.hue.js"]
    }
  ,"externally_connectable": {
    "matches": ["*://*.jldupont.com/*"
      , "*://*.systemical.com/*"
      , "*://*.c9users.io/*"
    ]
  }

}
